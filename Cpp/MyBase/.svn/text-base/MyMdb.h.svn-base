class CKeyClass;
#define KEY(key) CKeyClass key
#define FILED(filed) filed

class CKeyClass
{
public:
	CKeyClass():m_iKeyId(0){}
	CKeyClass(int iKeyId):m_iKeyId(iKeyId){}
	bool operator < (const CKeyClass& o) const
	{
		return true;
	}
	int m_iKeyId;
};

class CPocket
{
public:
CPocket(){}
CPocket(int iAcctId, int iAmount, int iDeductValue):m_acct_id(iAcctId), m_amount(iAmount), m_deduct_value(iDeductValue){}

KEY(m_acct_id),
FILED(int m_amount),
FILED(int m_deduct_value)
};


